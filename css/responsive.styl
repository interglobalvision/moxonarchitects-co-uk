/* iPads (portrait) ----------- */
/*for menu - new*/
@media all and (min-width: 1024px) {
	html,body {
		height:100%;
	}

	#menu {
		background:#eee;
	}

	/* hide open-button */
	a[href="#menu"] {
		display:none!important;
	}
}

/*other menu items */
/* Example for media query: change number of items per row */
@media screen and (max-width: 1190px) {
	#cbp-rfgrid li {
		width:50%;
	/* Fallback */
		width:-webkit-calc(100% / 2);
		width:calc(100% / 2);
	}
}

@media screen and (max-width: 945px) {
	#cbp-rfgrid li {
		width:50%;
	/* Fallback */
		width:-webkit-calc(100% / 2);
		width:calc(100% / 2);
	}
}

@media screen and (max-width: 660px) {
	#cbp-rfgrid li {
		width:50%;
	/* Fallback */
		width:-webkit-calc(100% / 2);
		width:calc(100% / 2);
	}

	#nextslide {
		display:none;
	}

	#prevslide {
		display:none;
	}
}

@media screen and (max-width: 660px) {
	#cbp-rfgrid li {
		width:100%;
	}
}

@media screen and (max-width: 400px) {
	#cbp-rfgrid li {
		width:100%;
	}
}

@media screen and (max-width: 300px) {
	#cbp-rfgrid li {
		width:100%;
	}
}

/* media queries */
@media screen and (max-width: 600px) {
	.menu-toggle,.main-navigation.toggled .nav-menu {
		display:block;
	}

	.main-navigation ul {
		display:none;
	}

	#mobile_nav {
		display:none;
	}
}

/*Fix Supersized on iOS*/
/* iPad [portrait + landscape] */
@media only screen and (min-device-width: 768px) and (max-device-width: 1024px) {
	#supersized {
		margin-left:0;
	}
}

@media only screen and (min-device-width : 768px) and (max-device-width : 1024px) and (orientation : portrait) {
	#supersized {
		width:110%!important;
	}
}

/* iPhone [portrait + landscape] */
@media only screen and (max-device-width: 480px) {
	#supersized {
		margin-left:0;
	}

	.menu_cont {
		display:none;
	}

	.project-content {
		display:none;
	}

	.contact_container {
		display:none;
	}

	.cat_list {
		display:none;
	}

	#content-mob {
		display:block;
		width:100%;
		position:relative;
		top:0;
		right:0;
		z-index:90000001;
	}

	#mob-content-nav {
		float:right;
	}

	.targetDiv {
		padding-top:35px;
		padding:15px;
		background-color:#000;
		background-color:rgba(0,0,0,.65);
	}

	.showSingle {
		padding:25px 0 25px 25px;
		text-align:center;
		background-color:#000;
		background-color:rgba(0,0,0,.5);
	}

	#mob-content-nav #button1,#button2 {
		border:none;
		float:right;
		margin-right:1px;
		font-size:12px;
		background-color:#000;
		display:block;
		color:#fff;
		margin-bottom:15px;
		-moz-border-radius:0;
		-webkit-border-radius:0;
		border-radius:0;
	}
}

/* Large screens ----------- */
@media only screen and (min-width : 1824px) {
	.content-dropdown {
		height:100%;
	}
}

@media (max-width: 1024px) {
	.primary-mobile-nav {
		display:none;
	}

	/*.menu_cont {display: none;}
	.cat_list{ display: none; }
	.project-content { display: none; }*/
	/* toggle */
	#slide-menu {
		margin-left:-250px;
		margin-top:-44px;
		left:0;
		width:250px;
		background:#222;
		background:rgba(0,0,0,0.5);
		position:fixed;
		height:100%;
		overflow-y:auto;
		z-index:300001;
		color:#ff0;
		font-weight:300;
	/*font-size: .7em;*/
	}

	.navigation {
		position:absolute;
		top:0;
		width:250px;
		list-style:none;
		margin:0;
		padding:0;
	}

	.navigation li {
		line-height:25px;
		text-indent:20px;
	}

	.navigation li a {
		color:#999;
		display:block;
		padding:3px 0;
		text-decoration:none;
		-webkit-transition:all .3s ease-out;
		-moz-transition:all .3s ease-out;
		-o-transition:all .3s ease-out;
		transition:all .3s ease-out;
	}

	.navigation li a:hover {
		color:#fff;
		background:rgba(255,255,0,.5);
		text-decoration:none;
		padding-left:20px;
	}

	.navigation li a:active,.navigation li a:focus {
		text-decoration:none;
	}

	#push {
		width:44px;
		height:34px;
		background:#000;
		background:rgba(0,0,0,0.5);
		text-align:center;
		padding-top:10px;
	}

	#mobile_nav {
	/*display: block !important;*/
		position:fixed;
		left:0;
		top:0;
		z-index:20002;
	}

	#push_content {
		width:44px;
		height:34px;
		background:#000;
		background:rgba(0,0,0,0.5);
		text-align:center;
		padding-top:10px;
	}

	#slide-menu-content {
		margin-right:-250px;
		margin-top:-98px;
		right:0;
		width:250px;
		background:#222;
		background:rgba(0,0,0,0.5);
		position:fixed;
		height:100%;
		overflow-y:auto;
		z-index:300001;
		color:#ff0;
		font-weight:300;
	/*font-size: .7em;*/
	}

	#mobile_content_nav {
		display:block!important;
		position:relative;
		z-index:20003;
		margin-top:10px;
	}
}

/* Smartphones (portrait and landscape) ----------- */
@media only screen and (min-device-width : 320px) and (max-device-width : 480px) {
	/* phone */
	.toggle-view-project-info-phone {
		display:block;
		list-style:none;
		margin-top:-50px;
		padding:0;
		width:100%;
		position:relative;
		z-index:100003;
	/*position: fixed; top:50px; left:0px; z-index: 10003; -- Disabled so shiftnav will work!*/
	}

	.toggle-view-project-info-phone li {
		margin:10px;
		position:relative;
		cursor:pointer;
	}

	.toggle-view-project-info-phone h3 {
		margin:0;
		color:#000;
		color:rgba(0,0,0,0);
		font-size:13px;
		text-transform:uppercase;
		padding:5px 10px;
		font-weight:400;
		width:85%;
		float:right;
	}

	h3.phone-proj-info {
		background:rgba(0,0,0,0);
		color:#fff;
		padding:0!important;
		color:#ff0;
		width:100%;
	}

	.toggle-view-project-info-phone span {
		position:absolute;
		right:10px;
		top:0;
		color:#ff0;
		font-size:16px;
	}

	.toggle-view-project-info-phone .panel {
		display:block;
		background:#000;
		background:#000;
		background:rgba(0,0,0,0.5);
		padding:15px 15px 15px 10px;
		height:415px;
		overflow:auto;
		font-size:14px;
		position:absolute;
		top:41px;
		left:-10px;
		width:100%;
	}

	.project-content-phone {
		height:418px;
		position:absolute;
		left:0;
		max-height:418px;
		padding:15px;
		overflow-y:auto;
	}

	/* contact page */
	.toggle-view-contact-info {
		list-style:none;
		margin:0;
		padding:0;
		width:400px;
		position:fixed;
		top:21px;
		left:-10px;
		z-index:10003;
	}

	.toggle-view-contact-info li {
		margin:10px;
		position:relative;
		cursor:pointer;
	}

	.toggle-view-contact-info h3 {
		background:#000;
		background:#000;
		background:rgba(0,0,0,0.5);
		margin:0;
		color:#fff;
		font-size:13px;
		text-transform:uppercase;
		padding:5px 10px;
		font-weight:400;
	}

	.toggle-view-contact-info span {
		position:absolute;
		right:10px;
		top:0;
		color:#ff0;
		font-size:16px;
	}

	.toggle-view-contact-info .panel {
		background:#000;
		background:#000;
		background:rgba(0,0,0,0.5);
		margin:1px 0;
		padding:15px 15px 15px 10px;
		height:518px;
		overflow:auto;
		font-size:14px;
		position:relative;
	}

	/* menu block */
	li.block-space a {
		opacity:0;
		cursor:none;
	}

	/*DT version*/
	.toggle-view-project-info,.toggle-view-all,.toggle-view-news,.toggle-view-project-cat,.toggle-view {
		display:none;
	}

	/* logo */
	.moxonlogogeneral {
		display:none;
		position:fixed;
		bottom:-125px!important;
		right:25px;
		z-index:-1!important;
	}

	.moxonlogo {
		display:none;
		position:fixed;
		top:10px;
		right:15px;
		z-index:1;
		width:200px;
		height:29px;
	}
}

